{
  "name": "portfolio",
  "homepage": "https://ajarana.github.io",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "moment": "^2.27.0",
    "node-sass-chokidar": "^1.5.0",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.8"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/sass/App.scss src/App.css",
    "watch-css": "npm run build-css && node-sass-chokidar src/sass/App.scss src/App.css --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy-gh-only": "gh-pages -d build -b master",
    "deploy": "npm run build && gh-pages -d build -b master"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
    "gh-pages": "^2.2.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
