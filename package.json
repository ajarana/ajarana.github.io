{
	"name": "portfolio-website",
	"version": "0.1.0",
	"homepage": "https://ajarana.github.io",
	"private": true,
	"dependencies": {
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/react": "^13.3.0",
		"@testing-library/user-event": "^14.2.3",
		"@types/jest": "^28.1.0",
		"@types/node": "^17.0.38",
		"@types/react": "^18.0.10",
		"@types/react-dom": "^18.0.5",
		"gh-pages": "^3.1.0",
		"moment": "^2.29.4",
		"normalize.css": "^8.0.1",
		"react": "^18.1.0",
		"react-dom": "^18.1.0",
		"react-router-dom": "^5.2.0",
		"react-scripts": "5.0.1",
		"typescript": "^4.7.2",
		"web-vitals": "^2.1.4"
	},
	"scripts": {
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build",
		"start": "react-scripts start",
		"build": "CI=true react-scripts build",
		"test": "react-scripts test",
		"test:staged": "react-scripts test --env=jsdom --watchAll=false --findRelatedTests",
		"eject": "react-scripts eject",
		"prepare": "husky install"
	},
	"eslintConfig": {
		"extends": "react-app",
		"rules": {
			"no-console": "warn"
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@types/react-router-dom": "^5.3.3",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier": "2.7.1",
		"sass": "^1.52.1"
	},
	"lint-staged": {
		"*.{json,css,scss,html,webmanifest,md}": "prettier --write",
		"*.{js,jsx,ts,tsx}": [
			"npm run test:staged",
			"eslint --max-warnings 0",
			"prettier --write"
		]
	}
}
